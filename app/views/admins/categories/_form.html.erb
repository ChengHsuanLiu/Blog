<div class="container">
	<div class="row">
		<div class="col s12">
			<div class="card">
				<div class="card-content">
					<%= form_for [:admins, @category] do |f| %>
						<div class="field margin-bottom-s">
							<%= f.label :name %>
							<%= f.text_field :name %>
						</div>
						<div class="field margin-bottom-s">
							<%= f.submit 'Submit', class: 'btn' %>
						</div>
					<% end %>
				</div>
			</div>
		</div>
	</div>
</div>
